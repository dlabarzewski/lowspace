      "model": ["PROPERTY"],
      "elmentRef": ["CONSTRUCTOR"],
      "renderer2": ["CONSTRUCTOR"]
    }
  },
  "external": {
    "httpClient": {
      "type": "HttpClient",
      "module": "@angular/common/http"
    },
    "router": {
      "type": "Router",
      "module": "@angular/router"
    },
    "activatedRoute": {
      "type": "ActivatedRoute",
      "module": "@angular/router"
    },
    "elementRef": {
      "type": "ElementRef",
      "module": "@angular/core"
    },
    "renderer2": {
      "type": "Renderer2",
      "module": "@angular/core"
    }
  },
  "entities": {
    "component": {
      "type": "ANGULAR_COMPONENT",
      "folder": "components/$name$",
      "config": {
        "stylesExtension": "none",
        "viewEncapsulation": "None",
        "selector": "generated",
        "changeDetectionStrategy": "OnPush"
      }
    },
    "model": {
      "type": "DATA_STRUCTURE"
    },
    "service": {
      "type": "ANGULAR_SERVICE"
    },
    "queryModel": {
      "type": "DATA_STRUCTURE"
    },
    "guard": {
      "id": "guard",
      "type": "ANGULAR_GUARD",
      "suffix": "Guard",
      "folder": "guards"
    },
    "interceptor": {
      "id": "interceptor",
      "type": "ANGULAR_INTERCEPTOR",
      "suffix": "Interceptor",
      "folder": "interceptors"
    },
    "pipe": {
      "id": "pipe",
      "type": "ANGULAR_PIPE",
      "suffix": "Pipe",
      "folder": "pipes"
    },
    "directive": {
      "id": "directive",
      "type": "ANGULAR_DIRECTIVE",
      "suffix": "Directive",
      "folder": "directives"
