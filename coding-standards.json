{
  "entityRelations": {
    "component": {
      "model": ["PROPERTY"],
      "service": ["CONSTRUCTOR"],
      "router": ["CONSTRUCTOR"],
      "activatedRoute": ["CONSTRUCTOR"]
    },
    "guard": {
      "model": ["PROPERTY"],
      "service": ["CONSTRUCTOR"],
      "router": ["CONSTRUCTOR"]
    },
    "service": {
      "httpClient": ["CONSTRUCTOR"],
      "model": ["PARAMETER", "RETURN", "PROPERTY"]
    },
    "pipe": {
      "httpClient": ["CONSTRUCTOR"],
      "model": ["PROPERTY"]
    },
    "directive": {
